/ {
    description = "WBMZ2-SUPERCAP";
    compatible-slots = "wb6-wbmz2-power";
	fragment {
		target-path = "/";
		
		__overlay__ {
			status = "okay";
		};
	};

	fragment-gpio {
		target-path = "/wirenboard/gpios";

		__overlay__ {
			SUPERCAP_PRESENT {
				io-gpios = <SLOT_GPIO_PORT_ALIAS(PIN1_SCL) SLOT_GPIO_PIN(PIN1_SCL) 1>;
				input;
				sort-order = <10001>;
			};
			SUPERCAP_DISCHARGING {
				io-gpios = <SLOT_GPIO(PIN2_SDA)>;
				input;
				sort-order = <10002>;
			};
		};
	};
};
