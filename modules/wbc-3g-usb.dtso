/ {
    description = "WBC-3G: 3G+2G Modem";
    compatible-slots = "wbc-usb";

    fragment_change_wb_env {
        target-path = "/wirenboard";

        __overlay__ {
            gsm {
                status = "okay";
                model = "sim5300e";
                type = "3g", "edge", "gprs";
            };
        };
    };
};
